{
  "manifest_version": 2,

  "name": "Diario de Yucatan RSS Feeder",
  "description": "RSS Feeder del Diaro de Yucatan",
  "default_locale": "es",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Diario de Yucatan"
  },
  "permissions": [
    "activeTab",
	"storage",
    "https://ajax.googleapis.com/"
  ]
}
